{
  "routes": [
    {
      "route": "/anonymous/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/authenticated/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/.auth/login/aad/callback",
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/.auth/refresh",
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/login",
      "redirect": "/.auth/login/aad"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout/aad"
    }
  ]
}
