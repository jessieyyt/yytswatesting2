{
  "routes": [
    {
      "route": "/anonymous/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/authenticated/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/logout",
      "redirect": "/.auth/aad/logout"
    },
    {
      "route": "/login",
      "redirect": "/.auth/aad/login"
    }
  ]
}
